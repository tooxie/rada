{
  "name": "gawshi",
  "author": "Alvaro Mouri√±o <alvaro@mourino.net>",
  "repository": "https://gitlab.com/mourino/gawshi",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0 <17.0.0"
  },
  "scripts": {
    "build": ". ./scripts/build.sh",
    "codegen": ". ./scripts/codegen.sh",
    "app:config": ". ./scripts/app_config.sh",
    "app:destroy": ". ./scripts/app_destroy.sh",
    "codegen:config": ". ./scripts/codegen_config.sh",
    "codegen:destroy": ". ./scripts/codegen_destroy.sh",
    "cognito:rootuser:config": ". ./scripts/rootuser_config.sh",
    "cognito:rootuser:destroy": ". ./scripts/rootuser_destroy.sh",
    "dev": "preact watch --https",
    "format": "prettier --write '*.{json,js}' 'src/**/*.{js,json,ts,tsx}'",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "serve": "sirv build --port 8080 --cors --single",
    "test": "jest ./tests"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@aws-crypto/sha256-browser": "^2.0.1",
    "@aws-sdk/client-s3": "3.137.0",
    "@aws-sdk/protocol-http": "^3.50.0",
    "@aws-sdk/s3-request-presigner": "^3.51.0",
    "@aws-sdk/url-parser": "^3.50.0",
    "@aws-sdk/util-format-url": "^3.50.0",
    "@babel/plugin-transform-react-jsx-source": "7.18.6",
    "amazon-cognito-identity-js": "^5.2.6",
    "aws-sdk": "2.1185.0",
    "babel-plugin-graphql-tag": "3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "graphql": "15.8.0",
    "graphql-tag": "^2.12.4",
    "jest": "^26.2.2",
    "jest-preset-preact": "^4.0.2",
    "preact": "10.10.0",
    "preact-cli": "3.4.1",
    "preact-cli-workbox-plugin": "^1.1.1",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "qr-scanner": "^1.4.1",
    "qrcode": "^1.5.0",
    "react-status-alert": "^1.1.2",
    "typescript": "4.6.3",
    "webpack-strip-block": "^0.3.0"
  },
  "devDependencies": {
    "@aws-amplify/cli": "^7.6.15",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-document-nodes": "1.17.12",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.8",
    "@types/qrcode": "^1.4.2",
    "@types/wicg-mediasession": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "amplify-codegen": "^4.0.1",
    "eslint": "^6.8.0",
    "eslint-config-preact": "^1.1.1",
    "prettier": "2.3.0",
    "qrcode-terminal": "^0.12.0",
    "sirv-cli": "^1.0.0-next.3",
    "yarn": "^1.22.19"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "preact",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "build/"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "error"
    }
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.ts",
      "<rootDir>/tests/__mocks__/setupTests.ts"
    ]
  },
  "prettier": {
    "printWidth": 90
  }
}
